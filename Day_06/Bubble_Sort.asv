%Bubble Sort-compare with adjacent element
arr=[64,34,25,12,22,11,9];
temp=0;
for i=1:length(arr)-1
    for j=i:length(arr)-1
        if(arr(j+1)<arr(j))
           temp=arr(j);
           arr(j)=arr(j+1);
           arr(j+1)=temp;
           disp(arr);
        end
       
    end
end

for i=1:length(arr)
  disp(arr(i));
end
